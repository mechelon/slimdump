{
  "name": "webfactory/slimdump",
  "description": "slimdump is a little tool to help you creating dumps of large MySQL-databases.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "webfactory GmbH",
      "email": "info@webfactory.de",
      "homepage": "http://www.webfactory.de",
      "role": "Developer"
    }
  ],
  "bin": [
    "bin/slimdump"
  ],
  "require": {
    "php": ">=7.2|^8.0",
    "ext-libxml": "*",
    "ext-pdo": "*",
    "ext-SimpleXML": "*",
    "doctrine/dbal": "^2.5.2",
    "fakerphp/faker": "^1.15",
    "symfony/console": "^6.0.0 || ^5.0.0 || ^4.0.0 || ^3.0.0 || ^2.8.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Webfactory\\Slimdump\\": "src/Webfactory/Slimdump"
    }
  },
  "config": {
    "sort-packages": true
  }
}
